{
  "name": "bp-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "_start": "webpack serve --progress --mode development",
    "start": "webpack serve --env NODE_ENV=development --env PCF_ENV=localDev --mode development --progress",
    "starts": "webpack --https --cert ./certs/server.cer --key ./certs/server.key --progress --mode development",
    "start:prod": "npm run server",
    "build": "webpack --mode production",
    "build:dev": "webpack --progress --mode development",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "server": "set PORT=5000&&node ./config/server.js",
    "lint": "eslint --ext .js,.jsx",
    "lint:fix": "eslint --fix --ext .js,.jsx",
    "lint:global": "npm run lint && npm run stylelint",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:update": "npm run test -- --u",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "stylelint": "stylelint **/*.scss **/*.css",
    "portinstall": "husky install",
    "docs": "jsdoc -c jsdoc.conf.json -R README.md -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdbudd/bp-react.git"
